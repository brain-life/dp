Universe = vanilla

+ProjectName="Diffusion-predictor"
Executable = run.sh
transfer_input_files = bin/Process_batch_data,input_data_$(batch).mat

should_transfer_files = YES

Output = log/$(batch).$(process).out 
Error = log/$(batch).$(process).err 
Log = log/$(batch).$(process).log 

stream_output = True

request_memory = 5G
requirements = OSGVO_OS_STRING == "RHEL 6" && Arch == "X86_64" && HAS_MODULES == True

arguments = $(batch) $(p1) $(p2) $(p3)

queue 
